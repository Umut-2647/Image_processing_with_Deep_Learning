    "import cv2\n",
    "import numpy as np"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 4,
   "metadata": {},
   "outputs": [],
   "source": [
    "img = cv2.imread('media/lenna.png')\n",
    "cv2.imshow('image', img)\n",
    "           \n",
    "hor = np.hstack((img,img))\n",
    "cv2.imshow('horizontal', hor)\n",
    "\n",
    "\n",
    "ver = np.vstack((img,img))\n",
    "cv2.imshow('vertical', ver)\n",
    "\n",
    "cv2.waitKey(0)\n",
    "cv2.destroyAllWindows()"
   ]
